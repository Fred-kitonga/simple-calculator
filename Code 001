#include <iostream>
#include <cmath>
using namespace std;
int main ()
{
 int num1, num2, result;
 char op;
 cout<<"enter the first number:";
 cin>>num1;
 cout<<"enter the second number:";
 cin>>num2;
 cout<<"enter the operator:";
 cin>>op;
 if(op=='+'){
    result= num1+ num2;
 } else if(op=='-'){
 result= num1- num2;
 }cout<<result;
 return 0;
 }
